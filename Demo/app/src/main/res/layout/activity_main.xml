<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="vertical"
    android:fadingEdge="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:context="com.tencent.mtt.tabcandroidsdk.MainActivity">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical">

            <TextView
                android:id="@+id/tv_param_exp_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="实验名称:"
                android:textSize="15dp" />

            <EditText
                android:id="@+id/et_param_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:background="@null"
                android:layout_marginLeft="10dp"
                android:hint="请输入实验名称..."
                android:layout_alignBottom="@+id/tv_param_exp_name"
                android:layout_toRightOf="@+id/tv_param_exp_name" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center_vertical">

            <TextView
                android:id="@+id/tv_param_gray_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="实验ID:"
                android:textSize="15dp" />

            <EditText
                android:id="@+id/et_gray_id_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:background="@null"
                android:layout_marginLeft="10dp"
                android:hint="请输入实验ID..."
                android:layout_alignBottom="@+id/tv_param_gray_id"
                android:layout_toRightOf="@+id/tv_param_gray_id" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center_vertical">

            <TextView
                android:id="@+id/tv_param_exp_event_code"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="事件code:"
                android:textSize="15dp" />

            <EditText
                android:id="@+id/et_event_code_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:background="@null"
                android:layout_marginLeft="10dp"
                android:hint="请输入事件code"
                android:layout_alignBottom="@+id/tv_param_exp_event_code"
                android:layout_toRightOf="@+id/tv_param_exp_event_code" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center_vertical">

            <TextView
                android:id="@+id/tv_param_user_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="用户ID:"
                android:textSize="15dp" />

            <EditText
                android:id="@+id/et_param_user_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15dp"
                android:background="@null"
                android:layout_marginLeft="10dp"
                android:hint="请输入切换用户ID...."
                android:layout_alignBottom="@+id/tv_param_user_id"
                android:layout_toRightOf="@+id/tv_param_user_id" />
        </RelativeLayout>


        <TextView
            android:id="@+id/tv_data_interface"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:gravity="center_horizontal"
            android:text="获取数据接口"
            android:textSize="15dp"
            android:textAllCaps="false"
            android:layout_marginBottom="5dp"
            android:textColor="@android:color/black" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/bt_ua_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:textAllCaps="false"
                android:layout_marginLeft="20dp"
                android:layout_marginBottom="5dp"
                android:text="getExpByName" />

            <Button
                android:id="@+id/bt_ua_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:textAllCaps="false"
                android:layout_marginBottom="5dp"
                android:text="getExpByNameWithExpose" />

        </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/bt_ua_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:layout_marginLeft="20dp"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="20dp"
            android:text="getAllExperiments" />

        <Button
            android:id="@+id/bt_ua_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="switchAccountId" />
    </LinearLayout>

    <TextView
        android:id="@+id/tv_content"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <TextView
        android:id="@+id/tv_report_interface"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textAllCaps="false"
        android:gravity="center_horizontal"
        android:text="上报接口"
        android:textSize="15dp"
        android:layout_marginBottom="5dp"
        android:textColor="@android:color/black" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/bt_report_1"
            android:layout_width="wrap_content"
            android:textAllCaps="false"
            android:layout_marginLeft="20dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="5dp"
            android:text="reportExpExpose" />


    </LinearLayout>

    <TextView
        android:id="@+id/tv_old_interface"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center_horizontal"
        android:text="老接口"
        android:textSize="15dp"
        android:layout_marginBottom="5dp"
        android:textColor="@android:color/black" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            android:id="@+id/bt_old_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:layout_marginLeft="20dp"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="20dp"
            android:text="getAllExpIds" />


        <Button
            android:id="@+id/bt_old_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="isFirstHitExpByName" />
    </LinearLayout>





    </LinearLayout>
</ScrollView>

